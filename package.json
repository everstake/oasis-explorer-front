{
  "name": "oasis-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test": "vue-cli-service test:unit",
    "test:watch": "vue-cli-service test:unit --watchAll",
    "lint": "vue-cli-service lint",
    "lint:scss": "stylelint ./src/{*,**/*}.{vue,scss}",
    "staging": "vue-cli-service build --mode staging",
    "report": "vue-cli-service build --report",
    "prepare": "husky install"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-brands-svg-icons": "^5.15.3",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@fortawesome/vue-fontawesome": "^0.1.9",
    "@hzzt/v-lazy-image": "^1.4.2",
    "axios": "^0.19.2",
    "bootstrap": "^4.6.0",
    "bootstrap-vue": "^2.21.2",
    "chart.js": "^2.9.3",
    "core-js": "^3.13.0",
    "dayjs": "^1.10.5",
    "lodash": "^4.17.21",
    "nprogress": "^0.2.0",
    "numeral": "^2.0.6",
    "qs": "^6.10.1",
    "uuid": "^8.3.2",
    "vue": "^2.6.11",
    "vue-chartjs": "^3.5.0",
    "vue-daterange-picker-light": "^0.3.5",
    "vue-notification": "^1.3.20",
    "vue-router": "^3.5.1",
    "vue-gtag": "^1.16.1",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~5.0.8",
    "@vue/cli-plugin-eslint": "~5.0.8",
    "@vue/cli-plugin-router": "~5.0.8",
    "@vue/cli-plugin-unit-jest": "~5.0.8",
    "@vue/cli-plugin-vuex": "~5.0.8",
    "@vue/cli-service": "~5.0.8",
    "@vue/eslint-config-airbnb": "^5.3.0",
    "@vue/test-utils": "^1.2.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-import": "^2.23.3",
    "eslint-plugin-vue": "^6.2.2",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "node-sass": "^4.12.0",
    "sass-loader": "^8.0.2",
    "stylelint": "^13.13.1",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-scss": "^3.19.0",
    "stylelint-webpack-plugin": "^2.1.1",
    "vue-template-compiler": "^2.6.11"
  },
  "lint-staged": {
    "*.{js,vue}": [
      "eslint --fix --max-warnings 0 --ignore-pattern '!.eslintrc.js'"
    ],
    "*.{scss,vue}": [
      "stylelint"
    ]
  }
}
